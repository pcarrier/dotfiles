HashKnownHosts yes
StrictHostKeyChecking no
ConnectTimeout 60
#Compression yes
#CompressionLevel 9

Host *
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa
  ServerAliveInterval 5
  AddressFamily inet
  Ciphers arcfour,blowfish-cbc
  ForwardAgent yes
  PreferredAuthentications publickey
  VerifyHostKeyDNS no

Host */*
  ProxyCommand ssh ${$(dirname %h)/\%%/@} nc -w5 ${$(basename %h)#*%%} %p

